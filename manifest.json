{
  "manifest_version": 2,

  "name": "SiteBlocker",
  "description": "This extension allows the user to block choosen sites from beeing loaded.",
  "version": "0.1",
  
  
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
	"default_icon": "on.png"
  },
  "permissions": [
    	"storage",
	"tabs"
  ]
}
